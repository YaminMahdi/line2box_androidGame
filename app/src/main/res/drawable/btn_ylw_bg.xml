<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/_18sdp" />
            <stroke android:width="@dimen/_2sdp" android:color="@color/yellowX" />
            <solid android:color="@color/yellowY" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/_18sdp" />
            <stroke android:width="@dimen/_2sdp" android:color="@color/yellowX" />
            <solid android:color="@color/yellowY" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/_18sdp" />
            <stroke android:width="@dimen/_2sdp" android:color="@color/yellowY" />
            <solid android:color="@color/yellowX" />
        </shape>
    </item>
</selector>